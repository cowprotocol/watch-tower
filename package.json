{
  "name": "@cowprotocol/watch-tower",
  "license": "GPL-3.0-or-later",
  "version": "2.11.1",
  "description": "A standalone watch tower, keeping an eye on Composable Cows  üëÄüêÆ",
  "author": {
    "name": "Cow Protocol"
  },
  "main": "dist/src/index.js",
  "scripts": {
    "build": "rimraf dist && rimraf src/types/generated && yarn typechain && tsc && cpx ./src/types/config.schema.json dist/src/types/config.schema.json",
    "fmt": "prettier --write \"src/**/*.ts\"",
    "lint": "eslint \"src/**/*.ts\" && prettier --check \"src/**/*.ts\"",
    "lint:fix": "eslint --fix \"src/**/*.ts\" && prettier --write \"src/**/*.ts\"",
    "test": "node --require ts-node/register --test \"src/**/*.{spec,test}.ts\"",
    "typechain": "typechain --target ethers-v5 --out-dir src/types/generated/ \"abi/*.json\"",
    "configschema": "node ./src/types/build.js",
    "prepare": "husky install && yarn typechain && yarn configschema",
    "cli": "ts-node src/index.ts"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.8.1",
    "@commitlint/config-conventional": "^19.8.1",
    "@typechain/ethers-v5": "^11.1.2",
    "@types/ajv": "^1.0.4",
    "@types/express": "^5.0.3",
    "@types/node": "^24.2.1",
    "@types/node-slack": "^0.0.34",
    "cpx": "^1.5.0",
    "dotenv": "^17.2.1",
    "eslint": "^9.33.0",
    "husky": "^9.1.7",
    "json-schema-to-typescript": "^15.0.4",
    "lint-staged": "^16.1.5",
    "prettier": "^3.6.2",
    "rimraf": "^6.0.1",
    "ts-node": "^10.9.2",
    "typechain": "^8.3.2",
    "typescript-eslint": "^8.39.1"
  },
  "dependencies": {
    "@commander-js/extra-typings": "^14.0.0",
    "@cowprotocol/contracts": "^1.8.0",
    "@cowprotocol/cow-sdk": "6.2.0-lens-bsc.0",
    "@typechain/ethers-v6": "^0.5.1",
    "ajv": "^8.17.1",
    "ajv-formats": "^3.0.1",
    "chalk": "^5.5.0",
    "ethers": "5.8.0",
    "express": "^5.1.0",
    "graphql-request": "^7.2.0",
    "level": "^10.0.0",
    "level-ts": "^2.1.0",
    "loglevel": "^1.9.2",
    "loglevel-plugin-prefix": "^0.8.4",
    "node-slack": "^0.0.7",
    "prom-client": "^15.1.3",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.2"
  }
}
